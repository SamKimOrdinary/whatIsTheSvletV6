<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>whatIsTheSvletV6</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>HelloSvlet</display-name>
    <servlet-name>HelloSvlet</servlet-name>
    <servlet-class>com.HelloSvlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloSvlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RequestSvlet</display-name>
    <servlet-name>RequestSvlet</servlet-name>
    <servlet-class>req.RequestSvlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RequestSvlet</servlet-name>
    <url-pattern>/reqsvlet</url-pattern>
  </servlet-mapping>
 
   <servlet>
    <description></description>
    <display-name>jdbcSvlet</display-name>
    <servlet-name>jdbcSvlet</servlet-name>
    <servlet-class>jndi.svc.JdbcSvlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>jdbcSvlet</servlet-name>
    <url-pattern>/jndisvlet</url-pattern>
  </servlet-mapping>
  
</web-app>